// In Servicenow, changes lockout value to true if user has not logged in to the system in 60 days
var users = new GlideRecord('sys_user');
//criteria below will be used to determine lockout criteria. Currently it is last login time of 60 days.
users.addEncodedQuery('last_login_time<javascript:gs.beginningOfLast60Days()^active=true^locked_out=false');
//add users to the below query to be added as an exemption.
users.addEncodedQuery('user_name!=admin^user_name!=APLnotifications@tactis.com^user_name!=support@earthclassmail.com^user_name!=no-reply@pmi-ops.org^user_name!=meawest.examiner2^user_name!=meawest.examiner1^user_name!=meaeast.examiner2^user_name!=meaeast.examiner1^user_name!=meacentral.examiner2^user_name!=meacentral.examiner1^user_name!=snc.emergency.tMbut^user_name!=virtual.agent^user_name!=instance.sec.user^user_name!=walkup^user_name!=ml_report.user^user_name!=ml.admin^user_name!=agent.intel.plugin^user_name!=bm.scheduler^user_name!=aoutier2@vibrenthealth.com^user_name!=securitycenter.user'); // exclude system users
users.query();

while (users.next()) {
    users.locked_out = true; // if user has not logged in 60 days after last login time or blank; check locked out
    users.update(); // update the profile

}
